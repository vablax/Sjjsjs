local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local gui = Instance.new("ScreenGui")
gui.Parent = player:WaitForChild("PlayerGui")

-- Function to create fling bars
local function createFlingBar(position)
    local bar = Instance.new("Frame")
    bar.Size = UDim2.new(0.2, 0, 0.05, 0)  -- Width and Height
    bar.Position = position
    bar.BackgroundColor3 = Color3.new(1, 0, 0) -- Red
    bar.Parent = gui
    
    -- Create invisible part
    local part = Instance.new("Part")
    part.Size = Vector3.new(10, 1, 2)
    part.Anchored = true
    part.CanCollide = false
    part.Transparency = 1
    part.Parent = workspace
    part.Position = Vector3.new(0, 5, 0)  -- Adjust position as needed
    
    -- Function to fling players
    part.Touched:Connect(function(hit)
        local character = hit.Parent
        local humanoidRoot = character and character:FindFirstChild("HumanoidRootPart")
        if humanoidRoot then
            local force = Vector3.new(math.random(-100, 100), 200, math.random(-100, 100))
            humanoidRoot.Velocity = force
        end
    end)
end

-- Create multiple fling bars
createFlingBar(UDim2.new(0.2, 0, 0.3, 0))
createFlingBar(UDim2.new(0.5, 0, 0.5, 0))
createFlingBar(UDim2.new(0.7, 0, 0.7, 0))
